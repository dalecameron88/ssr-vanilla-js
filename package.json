{
    "name": "express-demo",
    "version": "1.0.0",
    "description": "Express for SSR",
    "main": "index.js",
    "scripts": {
      "build": "rollup --config",
      "start": "node index",
      "dev": "nodemon index",
      "watch": "rollup --config --watch",
      "storybook": "start-storybook -p 6006",
      "build-storybook": "build-storybook",
      "test": "jest",
      "test:coverage": "jest --coverage",
      "test:output:watch": "jest --json --outputFile=.jest-test-results.json --watch",
      "develop": "concurrently \"npm run test:output:watch\" \"npm run storybook\""
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/chrisboakes/express-demo.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/chrisboakes/express-demo/issues"
    },
    "homepage": "https://github.com/chrisboakes/express-demo#readme",
    "dependencies": {
      "express": "^4.17.1",
      "express-handlebars": "^4.0.4",
      "node-fetch": "^2.6.0",
      "nodemon": "^2.0.3"
    },
    "devDependencies": {
      "@babel/core": "^7.10.2",
      "@storybook/addon-a11y": "^5.3.19",
      "@storybook/addon-docs": "^5.3.19",
      "@storybook/addon-jest": "^5.3.19",
      "@storybook/html": "^5.3.19",
      "@testing-library/jest-dom": "^5.10.1",
      "autoprefixer": "^9.8.0",
      "babel-loader": "^8.1.0",
      "cypress": "^4.8.0",
      "concurrently": "^5.2.0",
      "handlebars-loader": "^1.7.1",
      "jest": "^26.0.1",
      "jest-handlebars": "^1.0.1",
      "postcss-import": "^12.0.1",
      "rollup": "^2.10.4",
      "rollup-plugin-postcss": "^3.1.1"
    }
  }